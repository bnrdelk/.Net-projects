@model InternshipPlace
@{
    ViewData["Title"] = "Staj Yeri Detay";
}

<form asp-controller="Place" asp-action="Edit" method="post">
    <input type="hidden" asp-for="InternshipPlaceId">
    <div class="mb-3">
        <label asp-for="Title" class="form-label">Title</label>
        <input asp-for="Title" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Address" class="form-label">Address</label>
        <input asp-for="Address" class="form-control">
    </div>
    <button type="submit" class="btn btn-outline-success">Değişiklikleri Kaydet</button>
</form>