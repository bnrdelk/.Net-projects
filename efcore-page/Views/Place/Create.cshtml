@model InternshipPlace

@{
    ViewData["Title"] = "Staj Yeri Kaydı";
}

<form asp-action="Create" asp-controller="Place" method="post">
    <div class="mb-3">
        <label asp-for="Title" class="form-label">Staj Yeri İsmi</label>
        <input asp-for="Title" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Address" class="form-label">Staj Yeri Adresi</label>
        <input asp-for="Address" class="form-control">
    </div>

    <button type="submit" class="btn btn-outline-success">Kayıt Oluştur</button>
</form>